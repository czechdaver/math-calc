# Google AdSense
NEXT_PUBLIC_GOOGLE_ADSENSE_CLIENT_ID=ca-pub-XXXXXXXXXXXXXXXX

# Google Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Tag Manager
NEXT_PUBLIC_GTM_ID=GTM-XXXXXX

# Environment
NODE_ENV=development

# API Keys (if any)
# NEXT_PUBLIC_API_URL=https://api.example.com
# NEXT_PUBLIC_API_KEY=your_api_key_here

# Feature Flags
# NEXT_PUBLIC_ENABLE_EXPERIMENTAL_FEATURES=false

# Internationalization
# NEXT_PUBLIC_DEFAULT_LOCALE=cs
# NEXT_PUBLIC_SUPPORTED_LOCALES=cs,en,sk,pl,hu

# Analytics (optional)
# NEXT_PUBLIC_UMAMI_WEBSITE_ID=your_umami_id
# NEXT_PUBLIC_HOTJAR_ID=your_hotjar_id
# NEXT_PUBLIC_AMPLITUDE_API_KEY=your_amplitude_key

# Authentication (if needed)
# NEXT_PUBLIC_AUTH0_DOMAIN=your_domain.auth0.com
# NEXT_PUBLIC_AUTH0_CLIENT_ID=your_client_id
# NEXT_PUBLIC_AUTH0_AUDIENCE=your_audience

# Sentry (error tracking)
# NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Deployment
# NEXT_PUBLIC_VERCEL_ENV=development
# NEXT_PUBLIC_VERCEL_URL=your-vercel-url.vercel.app

# Cache (if using)
# CACHE_TTL=3600

# Rate Limiting (if applicable)
# RATE_LIMIT_WINDOW_MS=900000
# RATE_LIMIT_MAX_REQUESTS=100
